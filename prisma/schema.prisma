// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Person {
  id Int @id @default(autoincrement())
  name String
  cpf String @unique
  email String
  phone String
  image String?
  dateBirth DateTime
  createdAt DateTime @default(now()) @map("created_at")
  @@map("persons")
}

model Address{
  id Int @id @default(autoincrement())
  street String
  number String
  city String
  district String
  complement String
  CEP String

}
